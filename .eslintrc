{
    "parser": "babel-eslint",
    "env": {
        "amd": true,
        "browser": true,
        "mocha": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "comma-dangle": [
            2,
            "never"
        ],
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [
            2,
            "all"
        ],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [
            2,
            "both"
        ],
        "no-invalid-regexp": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "no-unexpected-multiline": 2,
        "block-scoped-var": 2,
        "consistent-return": 0,
        "default-case": 0,
        "guard-for-in": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": [
            2,
            {
                "boolean": true
            }
        ],
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-proto": 2,
        "no-redeclare": [
            2,
            {
                "builtinGlobals": true
            }
        ],
        "no-return-assign": [
            2,
            "except-parens"
        ],
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-throw-literal": 2,
        "no-useless-call": 2,
        "no-void": 2,
        "radix": 2,
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": [
            0,
            {
                "builtinGlobals": true,
                "hoist": "functions"
            }
        ],
        "array-bracket-spacing": [
            2,
            "never",
            {}
        ],
        "brace-style": [
            2,
            "1tbs",
            {
                "allowSingleLine": false
            }
        ],
        "no-empty": 2,
        "no-irregular-whitespace": 2,
        "valid-typeof": 2,
        "complexity": [
            2,
            5
        ],
        "curly": [
            2,
            "all"
        ],
        "eqeqeq": [
            2,
            "smart"
        ],
        "no-caller": 2,
        "no-extend-native": 2,
        "no-new": 2,
        "no-sequences": 2,
        "no-with": 2,
        "wrap-iife": [
            2,
            "inside"
        ],
        "strict": [
            2,
            "function"
        ],
        "no-undef": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "camelcase": 2,
        "new-cap": 2,
        "quotes": [
            2,
            "single"
        ],
        "max-depth": [
            2,
            2
        ],
        "max-len": [
            2,
            180,
            4,
            {}
        ],
        "max-params": [
            2,
            4
        ],
        "max-statements": [
            2,
            20
        ],
        "no-bitwise": 2
    }
}
